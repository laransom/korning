<!DOCTYPE html>
<head>
</head>

<body>

  <h1>Sales</h1>

  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Amount</th>
        <th>Invoice Number</th>
        <th>Units Sold</th>
        <th>Unit Price</th>
        <th>Invoice Frequency</th>
      </tr>
    </thead>

    <tbody>
      <% @sales.each do |sale| %>
      <tr>
        <td><%= sale.sale_date %></td>
        <td><%= sale.sale_amount %></td>
        <td></td>
        <td><%= sale.units_sold %></td>
        <td><%= sale.sale_amount/sale.units_sold %></td>
         <td></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</body>
